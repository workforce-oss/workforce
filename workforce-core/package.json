{
  "name": "workforce-core",
  "version": "0.1.0",
  "description": "Shared library for workforce",
  "author": "Jeff Joneson",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sedEffects": false,
  "engines": {
    "node": ">=20.17.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./model": {
      "import": {
        "types": "./dist/model.d.ts",
        "default": "./dist/model.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --exit tests/helpers/**/*.ts tests/objects/**/*.spec.ts tests/util/**/*.spec.ts",
    "test:file": "mocha --exit tests/helpers/*.ts",
    "test:mach": "tests/mach/mach.sh",
    "test:coverage": "nyc npm run test"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "reporter": [
      "text",
      "text-summary",
      "html",
      "lcov"
    ],
    "exclude": [
      "src/**/model.ts",
      "src/index.ts",
      "src/tests/**/*"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "report-dir": "coverage",
    "temp-dir": "coverage/.nyc_output"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/ansi-colors": "^3.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/handlebars": "^4.1.0",
    "@types/jira-client": "^7.1.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": ">=20.16.10",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.17",
    "@types/ws": "^8.5.5",
    "chai": "^4.3.7",
    "deep-equal-in-any-order": "^2.0.6",
    "eslint": "^9.12.0",
    "globals": "^15.9.0",
    "mocha": "10.7.3",
    "msw": "^1.2.2",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "nyc": "^17.1.0",
    "sinon": "^15.1.2",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google-cloud/speech": "^6.6.1",
    "@keycloak/keycloak-admin-client": "^26.0.0",
    "@kubernetes/client-node": "^0.22.1",
    "@octokit/auth-token": "^4.0.0",
    "@octokit/core": "^6.1.2",
    "@octokit/graphql": "^8.1.1",
    "@octokit/graphql-schema": "^15.25.0",
    "@octokit/plugin-rest-endpoint-methods": "^10.0.1",
    "@octokit/rest": "^21.0.2",
    "@octokit/webhooks": "^13.3.0",
    "@slack/socket-mode": "^1.3.6",
    "@slack/web-api": "^6.13.0",
    "ansi-colors": "^4.1.3",
    "async-lock": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.70",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-ws": "^5.0.2",
    "formdata-node": "^6.0.3",
    "groq-sdk": "^0.3.2",
    "handlebars": "^4.7.8",
    "https": "^1.0.0",
    "jira-client": "^8.2.2",
    "jose": "^4.15.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "openai": "^4.46.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.6",
    "tsm": "^2.3.0",
    "twilio": "^5.0.1",
    "universal-github-app-jwt": "^2.2.0",
    "weaviate-client": "^3.0.9",
    "weaviate-ts-client": "2.2.0",
    "ws": "^8.18.0"
  }
}
